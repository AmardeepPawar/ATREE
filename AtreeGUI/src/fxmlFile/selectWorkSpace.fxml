<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SelectWorkSpaceController">
   <children>
      <Label alignment="CENTER" prefHeight="47.0" prefWidth="408.0" text="Select Workspace" textFill="#2b28d3">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Label graphicTextGap="8.0" prefHeight="17.0" prefWidth="397.0" text="WorkSpace:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <TextField fx:id="workSpaceName" prefHeight="25.0" prefWidth="308.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <Button fx:id="DirectoryChooser" mnemonicParsing="false" prefHeight="25.0" prefWidth="57.0" text="Browse" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="408.0" spacing="20.0">
         <children>
            <Button fx:id="okbutton" mnemonicParsing="false" prefHeight="25.0" prefWidth="57.0" text="Ok" />
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="15.0" top="15.0" />
         </padding>
      </HBox>
      <HBox fx:id="labelHBox" alignment="TOP_CENTER">
         <children>
            <Label fx:id="noteId" alignment="TOP_LEFT" contentDisplay="TOP" graphicTextGap="0.0" textFill="#e80d0d">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <Label fx:id="selectStepMsgBx" alignment="TOP_LEFT" contentDisplay="TOP" graphicTextGap="0.0" />
         </children>
      </HBox>
   </children>
</VBox>
